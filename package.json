{
  "name": "stay-dripped-mobile-iv-website",
  "version": "1.0.0",
  "private": true,
  "description": "Modern static marketing website for Stay Dripped Mobile IV.",
  "scripts": {
    "dev": "webpack serve --config webpack.config.js --env development",
    "build": "npm run lint && webpack --config webpack.config.js --env production",
    "lint": "npm run lint:scss && npm run lint:js",
    "lint:scss": "stylelint \"scss/**/*.scss\"",
    "lint:js": "eslint \"js/**/*.js\"",
    "start": "npx serve .",
    "preview": "npx serve dist"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-scss": "^4.0.9",
    "purgecss-webpack-plugin": "^5.0.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "serve": "^14.2.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-scss": "^5.3.1",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.1",
    "workbox-webpack-plugin": "^7.0.0"
  }
